function valorDoConvenio (valor) {
    const padrao = 100
    switch (valor) {
        case 'crianca ate 10 anos':
            return padrao + 80
            
        case 'crianca maior de 10 até 30':
           return padrao + 50
           
        case 'maior que 30 ate 60':
           return padrao + 95
            
        case 'acima de 60':
           return padrao + 130
            
       }
}
console.log(valorDoConvenio('crianca ate 10 anos'))
console.log(valorDoConvenio('crianca maior de 10 até 30'))
console.log(valorDoConvenio('maior que 30 ate 60'))
console.log(valorDoConvenio('acima de 60'))