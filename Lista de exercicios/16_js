function calculadora (v1, operacao, v2) {
    switch (operacao) {
        case '+':
        return (v1 + v2)

        case '-':
        return (v1 - v2)

        case '*':
        return (v1 * v2)

        case '/':
        return (v1 / v2)

        default:
        return 'Operação inválida'
    }
}

console.log(calculadora(2, '+', 3))
console.log(calculadora(4, '-', 3))
console.log(calculadora(2, '*', 3))
console.log(calculadora(2, '/', 2))
console.log(calculadora(2, ',', 3))