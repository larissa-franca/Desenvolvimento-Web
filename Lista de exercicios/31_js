function negativos(vetor){
    let qtdNegativo = 0
        for (let i = 0; i < vetor.length; i++){
        if(vetor[i] < 0){
            qtdNegativo++
            }
        }
        return qtdNegativo
    }
    

vetor = [-1, -5, 0, 1]
vetor =  [10, 5, -7, 3, -1, 3, -11, -20, 6, 9]

console.log(negativos(vetor))