function ano(a) {
    if (a <= 0) {
        return false
    }else if (a % 400 == 0) {
       return true 
    }else if (a % 100 == 0){
        return false
    }else if (a % 4 == 0) {
        return true
    }else {
        return false
    }
    }

console.log(ano(4))
console.log(ano(400))
console.log(ano(2024))
console.log(ano(2016))
