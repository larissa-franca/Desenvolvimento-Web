function promocao(plano, salario) {
    switch (plano) {
        case 'a':
            return salario * 1.1

        case 'b':
            return salario * 1.15

        case 'c':
            return salario * 1.2

        default:
            return 'Plano inv√°lido'

    }
}

console.log(promocao('a', 1000))
console.log(promocao('b', 1000))
console.log(promocao('c', 1000))