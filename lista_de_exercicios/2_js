const tipoDeTriangulo = function(a, b, c) {
    if (a == b == c) {
        console.log('Equilatero')
    } else if (a == b && a != c && b != c) {
        console.log('Is√≥sceles')
    } else {
        console.log('Escaleno')
    }
}

tipoDeTriangulo(1, 1, 1)
tipoDeTriangulo(2, 2, 3)
tipoDeTriangulo(1, 2, 3)