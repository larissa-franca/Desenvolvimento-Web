a = 5
b = 2
function resultados1(a, b){
    return (a *= b)
}
function resultados2(a, b){
    return (a /= b)
}
function resultados3(a, b){
    return (a += b)
}
function resultados4(a, b){
    return (a -= b)
}

console.log('1: ', resultados1(a, b))
console.log('2: ', resultados2(a, b))
console.log('3: ', resultados3(a, b))
console.log('4: ', resultados4(a, b))