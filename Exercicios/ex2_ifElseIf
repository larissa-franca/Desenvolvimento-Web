function tipoDeTriangulo(a, b, c){
    if (a == b && b == c && a == c){
        return 'Equilatero'
    }else if(a == b && b != c || a != b && b == c || a == c && b != c){
        return 'Isosceles'
    }else{
        return 'Escaleno'
    }
}
console.log(tipoDeTriangulo(2, 2, 3))
console.log(tipoDeTriangulo(2, 4, 3))
console.log(tipoDeTriangulo(2, 2, 2))